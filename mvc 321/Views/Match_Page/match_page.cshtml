
@{
    ViewBag.Title = "match_page";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>match_page</title>
    <style>
       
    </style>
</head>
<body style="background-color: #85A3FF">
    <div id="match_detail">
        <h1>Match Detail</h1>
        </br>
        <div class="row">
            <div class="col-md-3">
                <p style="font-size:160%"><span class="label label-success"><b>Home Team</b></span></p>
                <p style="font-size:160%"><b><span class="label label-success"><b>Wollongong A</b></span></b></p>
            </div>
            <div class="col-md-2">
                <p style="font-size:160%"><b>VS</b></p>
                
            </div>
            <div class="col-md-2">
                <p style="font-size:160%"><span class="label label-warning"><b>Away Team</b></span></p>
                <p style="font-size:160%"><span class="label label-warning"><b>SYD B</b></span></p>
            </div>
        </div>
    <div>
            </br>
            <ul class="nav nav-tabs">
                <li class="active"><a data-toggle="tab" href="#home">Match ID</a></li>
                <li><a style="color:yellow" data-toggle="tab" href="#menu1">Time</a></li>
                <li><a style="color:yellow" data-toggle="tab" href="#menu2">Date</a></li>
                <li><a style="color:yellow" data-toggle="tab" href="#menu3">Location</a></li>
                <li><a style="color:yellow" data-toggle="tab" href="#menu4">Age group</a></li>
                <li><a style="color:yellow" data-toggle="tab" href="#menu5">Grade</a></li>
                <li><a style="color:yellow" data-toggle="tab" href="#menu6">Referee/s</a></li>
            </ul>

            <div class="tab-content">
                <div id="home" class="tab-pane fade in active">
                    <h3>ID:</h3>
                    <p><span class="label label-success">The match ID is 4394914 for this games.</span></p>
                </div>
                <div id="menu1" class="tab-pane fade">
                    <h3>Time:</h3>
                    <p><span class="label label-success">09:30AM - 11:30AM</span></p>
                </div>
                <div id="menu2" class="tab-pane fade">
                    <h3>Date:</h3>
                    <p><span class="label label-success">Feb/2/2016</span></p>
                </div>
                <div id="menu3" class="tab-pane fade">
                    <h3>Location:</h3>
                    <p><span class="label label-success">Wollongong Sports Center</span></p>
                </div>
                <div id="menu4" class="tab-pane fade">
                    <h3>Age group:</h3>
                    <p><span class="label label-success">18 years old</span></p>
                </div>
                <div id="menu5" class="tab-pane fade">
                    <h3>Grade:</h3>
                    <p><span class="label label-danger">Level A</span></p>
                </div>
                <div id="menu6" class="tab-pane fade">
                    <h3>Referee/s:</h3>
                    <p><span class="label label-primary">James bond(main)</span>
                        <span class="label label-default">Tony Stark</span>
                        <span class="label label-default"> Will Smith</span></p>
    </div>
    <div>
        ID
        ~
        Time
        Date
        Location
        Age
        Grade
        Home Team, Away Team
        Referee/s
    </div>
</body>
</html>
